(def fib [n]
  (if (< n 3)
    1
    (+ (fib (- n 1)) (fib (- n 2))))))

(write-line "fibonacci: " (fib 3))
(write-line "fibonacci: " (fib 4))
(write-line "fibonacci: " (fib 5))
(write-line "fibonacci: " (fib 6))
(write-line "fibonacci: " (fib 7))
(write-line "fibonacci: " (fib 13))
(write-line "fibonacci: " (fib 17))
(write-line "fibonacci: " (fib 21))
